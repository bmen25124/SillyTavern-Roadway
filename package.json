    "description": "A web application using pure JS and SCSS.",
    "main": "src/scripts/main.js",
    "scripts": {
        "dev": "cross-env NODE_ENV=development sass src/styles/main.scss dist/style.css --source-map & cross-env NODE_ENV=development webpack --mode development --watch",
